# --- Transport Options (Layanan) ---

tr:Transport_MRT_NS a tr:TransportOption ;
    tr:hasRoute tr:RoutePath_MRT_NS_NB ;
    tr:operatedBy tr:Op_MRTJakarta ;
    tr:price 14000 ; # Harga maksimal (variabel berdasarkan jarak)
    schema1:name "MRT North-South Line (Lebak Bulus - Bundaran HI)" .

tr:Transport_MRT_SN a tr:TransportOption ;
    tr:hasRoute tr:RoutePath_MRT_NS_SB ;
    tr:operatedBy tr:Op_MRTJakarta ;
    tr:price 14000 ; # Harga maksimal (variabel berdasarkan jarak)
    schema1:name "MRT North-South Line (Bundaran HI - Lebak Bulus)" .

# --- Routes (Jalur) ---

# Arah Utara (Northbound): Lebak Bulus -> Bundaran HI
tr:RoutePath_MRT_NS_NB a tr:Route ;
    tr:stopAt tr:Stop_MRT_01,
        tr:Stop_MRT_02,
        tr:Stop_MRT_03,
        tr:Stop_MRT_04,
        tr:Stop_MRT_05,
        tr:Stop_MRT_06,
