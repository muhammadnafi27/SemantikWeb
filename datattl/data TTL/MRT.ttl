        tr:Stop_MRT_12,
        tr:Stop_MRT_13 ;
    schema1:name "Rute Fisik MRT Lebak Bulus - Bundaran HI" .

# Arah Selatan (Southbound): Bundaran HI -> Lebak Bulus
tr:RoutePath_MRT_NS_SB a tr:Route ;
    tr:stopAt tr:Stop_MRT_13,
        tr:Stop_MRT_12,
        tr:Stop_MRT_11,
        tr:Stop_MRT_10,
